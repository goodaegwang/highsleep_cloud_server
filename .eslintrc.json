{
    "env": {
        "jest": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "import/newline-after-import": 0, // import, require 이후 반드시 개행하도록 하는 룰 끔
        "class-methods-use-this": 0, // 클래스 메소드 내부에서 반드시 this를 사용하도록 하는 룰 끔
        "import/no-dynamic-require": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "consistent-return": 0,
        "func-names": 0,
        "eqeqeq": 0,
        "no-dupe-keys": 0,
        "prefer-destructuring": 0,
        "no-useless-catch": 0,
        "no-dupe-else-if": 0,
        "padded-blocks": 0,
        "object-curly-newline": 0,
        "camelcase": 0,
        "no-shadow": 0,
        "no-plusplus": 0,
        "no-throw-literal": 0,
        "max-len": ["error", { "code": 200 }],
        "object-shorthand": 0,
        "prefer-const": 0,
        "no-new": 0,
        "no-constant-condition": 0
    }
}
